  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "^0.0.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "moment": "^2.24.0",
    "mongoose": "^5.3.16",
    "multer": "^1.4.1",
    "mysql2": "^1.6.4",
    "react": "^16.7.0",
    "react-bootstrap": "^0.32.4",
    "react-compound-timer": "^1.0.7",
    "react-dom": "^16.7.0",
